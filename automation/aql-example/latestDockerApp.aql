items.find (
    {
        "repo":{"$eq":"automation-docker-prod-local"},
        "@docker.repoName":{"$eq":"docker-app"},
        "@docker.manifest":{"$eq":"latest"},
        "@functional-test":{"$eq":"pass"},
        "$and":[
            {"property.key":{"$match":"xray*"}},
            {"property.value":{"$eq":"pending"}}
        ]
    }
).include("name", "@docker.repoName", "@docker.manifest", "artifact.module.build.name", "artifact.module.build.number")

