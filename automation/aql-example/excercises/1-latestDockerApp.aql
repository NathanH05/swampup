items.find (
    {
        "repo":{"$eq":"automation-docker-prod-local"},
        "@docker.repoName":{"$eq":"docker-app"},
        "$and":[
            {"property.key":{"$match":"xray*"}},
            {"property.value":{"$eq":"indexing"}}
        ]
    }
).include("name", "artifact.module.build.name", "artifact.module.build.number", "property.*")


